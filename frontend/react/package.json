  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "node -e \"const [major]=process.versions.node.split('.'); if(Number(major)!==22){console.error(`Detected Node ${process.versions.node}. Please use Node 22.x`); process.exit(1)}\"",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "cy:open": "cypress open",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:contract-types": "node --loader ts-node/esm src/scripts/generate-types-from-abi.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:headed": "cypress run --headed",
    "generate:contract-types": "node --loader ts-node/esm src/scripts/generate-types-from-abi.ts"
  },
  "dependencies": {
